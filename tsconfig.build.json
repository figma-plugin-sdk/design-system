{
    "compilerOptions": {
      "strict": true,
      "target": "ESNext",
      "outDir": "./dist",
      "module": "ESNext",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "resolveJsonModule": true,
      "lib": [
        "es2018",
        "dom"
      ],
      "paths": {
        "@fps/*": [
          "./components/*"
        ]
      }
    },
    "include": [
      "src/**/*.marko", "icons",
    ],
    "exclude": [
        "*.stories.ts"
    ]
  }